<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd 
    http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="true">

	<description>服务配置 </description>
    <!-- consumer application name -->
    <dubbo:application name="test-qida-common-serviceapp"  />
    <dubbo:consumer filter="InvocationRpcFilter" timeout="100000"></dubbo:consumer>
    <dubbo:registry address="zookeeper://192.168.3.124:2181" />
    
    <!-- 基础数据服务 -->
    <dubbo:reference id="areaService" interface="com.qida.systembasedata.service.AreaService" ></dubbo:reference>
    <dubbo:reference id="domainService" interface="com.qida.systembasedata.service.DomainService"></dubbo:reference>
    <dubbo:reference id="industryService" interface="com.qida.systembasedata.service.IndustryService"></dubbo:reference>
    
	<!-- 系统参数服务 -->
	<dubbo:reference id="systemParameterService" interface="com.qida.systemparameter.service.SystemParameterService"></dubbo:reference>
</beans>